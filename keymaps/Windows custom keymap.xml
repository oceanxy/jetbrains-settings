<keymap version="1" name="Windows custom keymap" parent="$default">
  <action id="$LRU">
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
  <action id="Editor LRU">
    <keyboard-shortcut first-keystroke="ctrl semicolon" />
  </action>
  <action id="Git.Clone">
    <keyboard-shortcut first-keystroke="shift ctrl alt o" />
  </action>
  <action id="Git.Configure.Remotes">
    <keyboard-shortcut first-keystroke="ctrl alt back_slash" />
  </action>
  <action id="Git.Pull">
    <keyboard-shortcut first-keystroke="shift ctrl t" />
  </action>
  <action id="GotoTest" />
  <action id="RevealIn">
    <keyboard-shortcut first-keystroke="shift ctrl alt e" />
  </action>
  <action id="ServiceView.ShowServices" />
  <action id="Vcs.VcsClone">
    <keyboard-shortcut first-keystroke="shift ctrl alt o" />
  </action>
  <action id="performancePlugin.ShowMemoryDialogAction">
    <keyboard-shortcut first-keystroke="shift ctrl alt m" />
  </action>
</keymap>